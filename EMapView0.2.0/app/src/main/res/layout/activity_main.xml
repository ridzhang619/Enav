<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.administrator.emapview.activity.MainActivity">

    <com.example.administrator.emapview.view.MatrixNavigateMapView
        android:id="@+id/enav_map_view"
        android:scaleType="matrix"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        />
    <!--<com.example.administrator.emapview.view.viewC-->
    <!--android:id="@+id/viewc"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:scaleType="matrix"-->
    <!--android:src="@drawable/abc"/>-->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:fitsSystemWindows="true"
        android:clipToPadding="true">
        <Button
            android:id="@+id/handle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Handle"/>
        <Button
            android:id="@+id/draw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Line"/>
        <Button
            android:id="@+id/draw1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Polygon"/>
        <Button
            android:id="@+id/draw2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Circle"/>
        <Button
            android:id="@+id/draw3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vessel"/>
        <Button
            android:id="@+id/draw4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vessel1"/>
        <Button
            android:id="@+id/draw5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DesSub"/>
    </LinearLayout>

